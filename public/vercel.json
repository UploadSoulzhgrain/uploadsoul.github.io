{
  "version": 2,
  "framework": null,
  "installCommand": "npm install -g pnpm@8.10.2 && pnpm install --no-frozen-lockfile",
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "github": {
    "silent": true,
    "autoAlias": true
  }
}