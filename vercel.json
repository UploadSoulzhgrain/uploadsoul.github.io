{
  "version": 2,
  "buildCommand": "chmod +x ./vercel-build.sh && ./vercel-build.sh",
  "framework": "vite",
  "installCommand": "npm install -g pnpm@8.10.2 && pnpm install --no-frozen-lockfile",
  "outputDirectory": "dist",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "packageManager": "pnpm@8.10.2"
}