{
  "version": 2,
  "framework": "vite",
  "cleanUrls": true,
  "routes": [
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml",
      "headers": {
        "Content-Type": "application/xml"
      }
    },
    {
      "src": "/sitemap.xsl",
      "dest": "/sitemap.xsl",
      "headers": {
        "Content-Type": "text/xsl"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}